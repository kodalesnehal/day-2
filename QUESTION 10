    <button type="button" onclick="check(mobile)"> click to check</button>
    <script>
        function check (mobile)
        {
           if(mobile.length==10)
           {
               document.write("mobile number is ok")
           }
           else{
               document.write("not ok");
           }
            
        }
        var mobile="2321232562";

    </script>

            

